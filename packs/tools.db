{"name":"Abacus","permission":{"default":0,"tfyU1Pxc1Hi3HECF":3},"type":"trapping","data":{"description":{"type":"String","label":"Description","value":"<p>An abacus is a device used to make arithmetic calculations. It is a frame set with rods on which balls or beads slide. This is a common tool for merchants.</p>"},"gmdescription":{"type":"String","label":"Description","value":""},"quantity":{"type":"Number","label":"Quantity","value":1},"encumbrance":{"type":"Number","label":"Encumbrance","value":5},"price":{"type":"String","label":"Price","gc":"10","ss":"0","bp":"0"},"availability":{"type":"String","label":"Availability","value":"rare"},"location":{"type":"Number","label":"Location","value":0},"trappingType":{"type":"String","label":"Trapping Type","value":"toolsAndKits"},"worn":false,"spellIngredient":{"type":"String","value":""}},"flags":{},"img":"systems/wfrp2e/icons/blank.png","_id":"5c2HEPcXC3X8mpuY"}
